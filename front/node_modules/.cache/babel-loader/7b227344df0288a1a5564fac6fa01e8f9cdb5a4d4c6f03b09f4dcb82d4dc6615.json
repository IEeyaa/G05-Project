{"ast":null,"code":"import { reactive, toRefs } from \"@vue/reactivity\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  setup() {\n    const registerForm = reactive({\n      rEmail: \"\",\n      rPassword: \"\",\n      confirmPassword: \"\",\n      identifyCode: \"\",\n      rAgree: 0\n    });\n    // 方法\n    // 注册\n    function register() {\n      console.log(\"注册\", registerForm);\n    }\n    // 获取验证码\n    function getIdentifyCode() {\n      console.log(\"获取验证码\");\n    }\n    // 确认密码\n    // function confirmFunc() {\n    //   if (registerForm.confirmPassword !== registerForm.rPassword)\n    //     alert(\"密码与确认密码不一致\");\n    // }\n    const confirmFunc = () => {\n      if (registerForm.confirmPassword !== registerForm.rPassword) ElMessage.error(\"密码与确认密码不一致.\");\n    };\n    return {\n      ...toRefs(registerForm),\n      register,\n      getIdentifyCode,\n      confirmFunc\n    };\n  }\n};","map":{"version":3,"mappings":"AAmDA,SAASA,QAAQ,EAAEC,MAAK,QAAS,iBAAiB;AAClD,SAASC,SAAQ,QAAS,cAAc;AACxC,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,YAAW,GAAIJ,QAAQ,CAAC;MAC5BK,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,EAAE;MACbC,eAAe,EAAE,EAAE;MACnBC,YAAY,EAAE,EAAE;MAChBC,MAAM,EAAE;IACV,CAAC,CAAC;IACF;IACA;IACA,SAASC,QAAQ,GAAG;MAClBC,OAAO,CAACC,GAAG,CAAC,IAAI,EAAER,YAAY,CAAC;IACjC;IACA;IACA,SAASS,eAAe,GAAG;MACzBF,OAAO,CAACC,GAAG,CAAC,OAAO,CAAC;IACtB;IACA;IACA;IACA;IACA;IACA;IACA,MAAME,WAAU,GAAI,MAAM;MACxB,IAAIV,YAAY,CAACG,eAAc,KAAMH,YAAY,CAACE,SAAS,EACzDJ,SAAS,CAACa,KAAK,CAAC,aAAa,CAAC;IAClC,CAAC;IACD,OAAO;MACL,GAAGd,MAAM,CAACG,YAAY,CAAC;MAEvBM,QAAQ;MACRG,eAAe;MACfC;IACF,CAAC;EACH;AACF,CAAC","names":["reactive","toRefs","ElMessage","setup","registerForm","rEmail","rPassword","confirmPassword","identifyCode","rAgree","register","console","log","getIdentifyCode","confirmFunc","error"],"sourceRoot":"","sources":["E:\\课程\\大三上\\软件工程管理\\Code\\G05-Project\\front\\src\\views\\RegisterView.vue"],"sourcesContent":["<template>\r\n  <body id=\"poster\">\r\n    <div\r\n      class=\"layout\"\r\n      style=\"margin-left: 40px; box-shadow: 12px 12px 12px rgba(0, 0, 0, 0.1)\"\r\n    >\r\n      <h1>Sign up</h1>\r\n      <el-form\r\n        label-position=\"left\"\r\n        label-width=\"100px\"\r\n        style=\"max-width: 460px\"\r\n        class=\"loginForm\"\r\n      >\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"rName\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱：\">\r\n          <el-input v-model=\"rEmail\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"密码：\">\r\n          <el-input type=\"password\" v-model=\"rPassword\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"确认密码：\">\r\n          <el-input\r\n            type=\"password\"\r\n            v-model=\"confirmPassword\"\r\n            @blur=\"confirmFunc\"\r\n          />\r\n        </el-form-item>\r\n        <el-row>\r\n          <el-checkbox\r\n            class=\"checkBox\"\r\n            v-model=\"rAgree\"\r\n            label=\"同意用户使用准则\"\r\n            name=\"type\"\r\n          />\r\n        </el-row>\r\n        <el-button\r\n          v-if=\"rAgree\"\r\n          type=\"primary\"\r\n          class=\"loginBtn\"\r\n          @click=\"register\"\r\n        >\r\n          注册\r\n        </el-button>\r\n      </el-form>\r\n    </div>\r\n  </body>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from \"@vue/reactivity\";\r\nimport { ElMessage } from \"element-plus\";\r\nexport default {\r\n  setup() {\r\n    const registerForm = reactive({\r\n      rEmail: \"\",\r\n      rPassword: \"\",\r\n      confirmPassword: \"\",\r\n      identifyCode: \"\",\r\n      rAgree: 0,\r\n    });\r\n    // 方法\r\n    // 注册\r\n    function register() {\r\n      console.log(\"注册\", registerForm);\r\n    }\r\n    // 获取验证码\r\n    function getIdentifyCode() {\r\n      console.log(\"获取验证码\");\r\n    }\r\n    // 确认密码\r\n    // function confirmFunc() {\r\n    //   if (registerForm.confirmPassword !== registerForm.rPassword)\r\n    //     alert(\"密码与确认密码不一致\");\r\n    // }\r\n    const confirmFunc = () => {\r\n      if (registerForm.confirmPassword !== registerForm.rPassword)\r\n        ElMessage.error(\"密码与确认密码不一致.\");\r\n    };\r\n    return {\r\n      ...toRefs(registerForm),\r\n\r\n      register,\r\n      getIdentifyCode,\r\n      confirmFunc,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.layout {\r\n  position: absolute;\r\n  top: 20%;\r\n  left: calc(50% - 300px);\r\n  border-radius: 15px;\r\n  background-clip: padding-box;\r\n  margin: 90px auto;\r\n  width: 450px;\r\n  padding: 35px 35px 15px 35px;\r\n  background: #fff;\r\n  border: 1px solid #eaeaea;\r\n  box-shadow: 0 0 25px #cac6c6;\r\n}\r\n\r\n/* #poster {\r\n  background: url(\"../pictures/login.jpg\") no-repeat;\r\n  background-position: center;\r\n  height: 100%;\r\n  width: 100%;\r\n  background-size: cover;\r\n  position: absolute;\r\n} */\r\nbody {\r\n  margin: 0px;\r\n  padding: 0;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}